{
  "mcpServers": {
    "qms": {
      "command": "python3",
      "args": [
        "/proxy/mcp_proxy.py",
        "http://host.docker.internal:8000/mcp",
        "--retries", "3",
        "--timeout", "10000",
        "--header", "X-API-Key=qms-internal",
        "--header", "Authorization=Bearer internal-trusted"
      ]
    },
    "git": {
      "command": "python3",
      "args": [
        "/proxy/mcp_proxy.py",
        "http://host.docker.internal:8001/mcp",
        "--retries", "3",
        "--timeout", "10000",
        "--header", "X-API-Key=git-internal",
        "--header", "Authorization=Bearer internal-trusted"
      ]
    }
  }
}
